<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>AIESEC in Romania Schimb de Oameni intre Locale</title>

    <!-- Bootstrap Core CSS - Uses Bootswatch Flatly Theme: http://bootswatch.com/flatly/ -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="css/freelancer.css" rel="stylesheet">
    <link href="css/aiesec.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body id="page-top" class="index">

    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#page-top">AIESEC in Romania SOL</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <li class="page-scroll">
                        <a href="#portfolio">Trainers</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#about">Request</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#contact">Contact</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>

    <!-- Header -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="img/romania.png" alt="Romanian Flag">
                    <div class="intro-text">
                        <span class="name"> </span>
                        <hr class="star-light">
                        <span class="skills">Want to make exchange happen?<br />We can help.</span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Portfolio Grid Section -->
    <section id="portfolio">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>SOL Trainers</h2>
                    <hr class="star-primary">
                </div>
            </div>

            <div class="row" id="training-areas">
                <div class="col-lg-12 text-center">
                    <p>My trainer needs to know...</p>
                    <% training_areas.each do |area| %>
                        <button type="button" <%= "id='#{ area }'"%> class="btn btn-info btn-sm area-btn" data-toggle="button" ><%= area %></button>
                    <% end %>
                </div>
            </div>

            <br />

            <div class="row" id="trainers">
                <% i = 0 %>
                <% trainers.each do |trainer| %>
                    <% i = i + 1 %>
                    <div class="portfolio-item col-sm-4" <%= "data-id='#{ trainer.name }'" %> >
                        <a <%= "href='#portfolioModal#{ i }'" %> class="portfolio-link" data-toggle="modal">
                            <div class="caption">
                                <div class="caption-content">
                                    <i class="fa fa-search-plus fa-3x"></i><br />
                                    <%= trainer.name %><br />
                                    <%= trainer.local_committee %><br />
                                </div>
                            </div>
                            <img <%= "src='img/trainer_photos/#{ trainer.photo_name }'" %> class="podio-img img-centered img-responsive" <%= "alt='#{ trainer.to_s }'" %>>
                        </a>
                    </div>
                <% end %>
            </div>
        </div>
    </section>

    <!-- For use with quicksand plugin -->
    <div class="hidden row" id="new-trainers">
    </div>

    <div class="hidden row" id="all-trainers">
        <% i = 0 %>
            <% trainers.each do |trainer| %>
                <% i = i + 1 %>
                <div class="col-sm-4 portfolio-item" <%= "data-id='#{ trainer.name }'" %> >
                    <a <%= "href='#portfolioModal#{ i }'" %> class="portfolio-link" data-toggle="modal">
                        <div class="caption">
                            <div class="caption-content">
                                <i class="fa fa-search-plus fa-3x"></i><br />
                                <%= trainer.name %><br />
                                <%= trainer.local_committee %><br />
                            </div>
                        </div>
                        <img <%= "src='img/trainer_photos/#{ trainer.photo_name }'" %> class="podio-img img-centered img-responsive" <%= "alt='#{ trainer.to_s }'" %>>
                    </a>
                </div>
        <% end %>
    </div>

    <!-- About Section -->
    <section class="success" id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>Request a Trainer</h2>
                    <hr class="star-light">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <p>All training requests are managed through the AIESEC in Romania SOL Podio space.  When requesting a training, you must be sure to check Podio regularly because that is where all communication will happen.</p>
                    <p>If you have any questions about requesting a trainer or need advice about who you should request you can contact Michael Jalkio, MCVP Organizational Development.</p>
                </div>
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <a href="https://podio.com/webforms/15599874/1045454" class="btn btn-lg btn-podio btn-outline">
                        <i class="fa fa-users"></i> Go to Podio
                    </a>
                </div>
            </div>
        </div>
    </section>

    <!-- Contact Section -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <img class="img-responsive img-centered" src="img/builder.png" alt="Can he fix it?  Yes he can!">

                    <h2>Questions?</h2>
                    <hr class="star-primary">
                    <p>If you are having issues with this website, contact <a href="mailto:michael.jalkio@aiesec.net"><b>Michael Jalkio</b></a>.</p>
                </div>
            </div>
        </div>
    </section>

    <!-- Footer -->
    <footer class="text-center">
        <div class="footer-above">
            <div class="container">
                <div class="row">
                    <div class="footer-col col-md-12">
                        <h3>AIESEC Around the Web</h3>
                        <ul class="list-inline">
                            <li>
                                <a href="http://aiesec.ro/" class="btn-social btn-outline"><i class="fa fa-fw fa-globe"></i></a>
                            </li>

                            <li>
                                <a href="https://www.facebook.com/AIESECinRomania" class="btn-social btn-outline"><i class="fa fa-fw fa-facebook"></i></a>
                            </li>
                            <li>
                                <a href="https://twitter.com/aiesecromania" class="btn-social btn-outline"><i class="fa fa-fw fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com/aiesecromania" class="btn-social btn-outline"><i class="fa fa-fw fa-instagram"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-below">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        Copyright &copy; Michael Jalkio 2015
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- Scroll to Top Button (Only visible on small and extra-small screen sizes) -->
    <div class="scroll-top page-scroll visible-xs visible-sm">
        <a class="btn btn-primary" href="#page-top">
            <i class="fa fa-chevron-up"></i>
        </a>
    </div>

    <!-- Portfolio Modals -->
    <% i = 0 %>
    <% trainers.each do |trainer| %>
        <% i = i + 1 %>
        <div class="portfolio-modal modal fade" <%= "id='portfolioModal#{ i }'" %> tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-content">
                <div class="close-modal" data-dismiss="modal">
                    <div class="lr">
                        <div class="rl">
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-8 col-lg-offset-2">
                            <div class="modal-body">
                                <h2 ><%= trainer.name %></h2>
                                <hr class="star-primary">
                                <img <%= "src='img/trainer_photos/#{ trainer.photo_name }'" %> class="img-centered img-responsive" <%= "alt='#{ trainer.to_s }'" %>>

                                <% unless trainer.badges.empty? %>
                                    <h4>Badges</h4>
                                    <ul class="list-inline">
                                        <% trainer.badges.each do |badge| %>
                                            <li>
                                                <img class="img-responsive" <%= "src='img/#{ badge.image_name }'"%> <%= "alt='#{ badge.name }'" %> data-toggle="tooltip" data-placement="bottom" <%= "title='#{ badge.name }'" %>>
                                            </li>
                                        <% end %>
                                    </ul>
                                <% end %>

                                <h4>Local Committee</h4>
                                <p class="text-info"><%= trainer.local_committee %></p>

                                <h4>Current Position</h4>
                                <p class="text-info"><%= trainer.current_position %></p>

                                <h4>Functional Expertise In</h4>
                                <ul class="list-unstyled text-info">
                                    <% trainer.functions_can_train_in.each do |function| %>
                                        <li><%= function %></li>
                                    <% end %>
                                </ul>

                                <h4>Number of Trainings Delivered</h4>
                                <p class="text-info"><%= trainer.num_trainings %></p>

                                <br />
                                <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <% end %>

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="js/classie.js"></script>
    <script src="js/cbpAnimatedHeader.js"></script>

    <!-- Contact Form JavaScript -->
    <script src="js/jqBootstrapValidation.js"></script>
    <script src="js/contact_me.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="js/freelancer.js"></script>
    <script src="js/aiesec.js"></script>
    <script src="js/jquery.quicksand.js"></script>

</body>

</html>
